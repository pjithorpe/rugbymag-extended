{
    "name": "Rugby Magazine Fantasy Extended",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "settings_popup.html"
    },
    "content_scripts": [{
        "matches": ["https://therugbymagazine.com/fantasy*team*gameweek*"],
        "js": ["totalScores.js"]
    }]
}